{
  "name": "swolemate",
  "version": "0.0.1",
  "description": "SwoleMate, the ultimate lifting companion",
  "main": "server.js",
  "scripts": {
    "start": "next",
    "build": "next build",
    "dev": "nodemon server.js",
    "docker:build": "docker build -t swolemate .",
    "docker:clean": "docker rm -f swolemate || true",
    "docker:run": "docker run -it -v ${PWD}:/usr/src/app -v /usr/src/app/node_modules -p 3000:3000 swolemate && npm run docker:logs",
    "docker:stop": "docker stop swolemate",
    "docker:start": "docker start swolemate && npm run docker:logs",
    "docker:logs": "docker logs -f swolemate",
    "deploy": "npm run docker:build && npm run docker:clean && npm run docker:run"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "next": "^6.1.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "nodemon": "^1.17.5"
  }
}
